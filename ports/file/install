VERSION="5.41"
SRC_FILENAME="file-"$VERSION".tar.gz"
URL="http://ftp.astron.com/pub/file/"$SRC_FILENAME

function configure_port() {
	./configure								\
		--host=$HOST							\
		--prefix="/usr"
}

function build_port() {
	make -j$NJOBS V=1
}

function install_port() {
	make install DESTDIR=$SYSROOT
}
