URL="http://ftp.astron.com/pub/file/"
VERSION="5.41"
SRC_FILENAME="file-"$VERSION".tar.gz"

function configure_port() {
	./configure								\
		--host=$TARGET							\
		--prefix="/usr"							\
		CFLAGS="--static" LDFLAGS="--static"
}

function build_port() {
	make -j$NJOBS V=1
}

function install_port() {
	make install DESTDIR=$SYSROOT
}
