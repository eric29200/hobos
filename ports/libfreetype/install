URL="https://download.savannah.gnu.org/releases/freetype/"
VERSION="2.12.1"
SRC_FILENAME="freetype-"$VERSION".tar.xz"

function configure_port() {
	./configure								\
		--host=$TARGET							\
		--prefix="/"							\
		CC=$CC
}

function build_port() {
	make -j$NJOBS CC=$CC
}

function install_port() {
	make install DESTDIR=$MUSL_DIR
}
