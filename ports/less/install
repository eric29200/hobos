URL="https://ftp.gnu.org/gnu/less/"
VERSION="590"
SRC_FILENAME="less-"$VERSION".tar.gz"

function configure_port() {
	./configure								\
		--host=$TARGET							\
		--prefix=$SYSROOT						\
		CC=$MUSL_CC LD=$MUSL_CC CFLAGS="-static" LDFLAGS="-static"
}

function build_port() {
	make -j$NJOBS
}

function install_port() {
	make install
}