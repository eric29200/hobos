URL="https://github.com/libexif/libexif/archive/refs/tags/"
VERSION="0.6.24"
SRC_FILENAME="v"$VERSION".tar.gz"

function configure_port() {
	autoreconf -i
	./configure						\
		--host=$TARGET					\
		--prefix=$SYSROOT
}

function build_port() {
	make -j$NJOBS
}

function install_port() {
	make install
}