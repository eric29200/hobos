VERSION="2.3"
SRC_FILENAME="inetutils-"$VERSION".tar.xz"
URL="https://ftp.gnu.org/gnu/inetutils/"$SRC_FILENAME

function configure_port() {
	./configure								\
		--host=$HOST							\
		--disable-threads						\
		--prefix="/usr"
}

function build_port() {
	make -j$NJOBS
}

function install_port() {
	make install DESTDIR=$SYSROOT
}
