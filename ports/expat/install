URL="https://github.com/libexpat/libexpat/releases/download/R_2_4_9/"
VERSION="2.4.9"
SRC_FILENAME="expat-"$VERSION".tar.bz2"

function configure_port() {
	./configure						\
		--host=$HOST					\
		--prefix=$SYSROOT
}

function build_port() {
	make -j$NJOBS
}

function install_port() {
	make install
}
