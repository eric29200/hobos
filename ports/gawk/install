URL="https://ftp.gnu.org/gnu/gawk/"
VERSION="5.2.0"
SRC_FILENAME="gawk-"$VERSION".tar.gz"

function configure_port() {
	./configure								\
		--host=$TARGET							\
		--disable-nls							\
		--prefix="/usr"
}

function build_port() {
	make -j$NJOBS
}

function install_port() {
	make install DESTDIR=$SYSROOT
}
