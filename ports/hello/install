URL="https://ftp.gnu.org/gnu/hello/"
VERSION="2.12.1"
SRC_FILENAME="hello-"$VERSION".tar.gz"

function configure_port() {
	./configure								\
		--host=$TARGET							\
		--disable-nls							\
		--disable-threads						\
		--prefix=$SYSROOT						\
		CC=$MUSL_CC LD=$MUSL_CC CFLAGS="-static" LDFLAGS="-static"
}

function build_port() {
	make -j$NJOBS
}

function install_port() {
	make install
}