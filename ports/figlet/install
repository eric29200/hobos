URL="http://ftp.figlet.org/pub/figlet/program/unix/"
VERSION="2.2.5"
SRC_FILENAME="figlet-"$VERSION".tar.gz"

function configure_port() {
	:
}

function build_port() {
	make -j$NJOBS CC=$MUSL_CC LD=$MUSL_CC CFLAGS="-static" LDFLAGS="-static"
}

function install_port() {
	make install DESTDIR=$ROOT_DIR
}