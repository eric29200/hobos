VERSION="1.6.39"
SRC_FILENAME="libpng-"$VERSION".tar.xz"
URL="https://downloads.sourceforge.net/libpng/"$SRC_FILENAME

function configure_port() {
	./configure								\
		--host=$HOST							\
		--prefix=$SYSROOT"/usr"
}

function build_port() {
	make -j$NJOBS
}

function install_port() {
	make install
}
