URL="https://ftp.gnu.org/gnu/tar/"
VERSION="1.34"
SRC_FILENAME="tar-"$VERSION".tar.xz"

function configure_port() {
	./configure								\
		--host=$HOST							\
		--disable-nls							\
		--prefix="/usr"
}

function build_port() {
	make -j$NJOBS
}

function install_port() {
	make install DESTDIR=$SYSROOT
}
