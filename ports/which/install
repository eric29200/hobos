URL="https://ftp.gnu.org/gnu/which/"
VERSION="2.21"
SRC_FILENAME="which-"$VERSION".tar.gz"

function configure_port() {
	./configure								\
		--host=$TARGET							\
		--prefix="/usr"							\
		CC=$MUSL_CC LD=$MUSL_CC CFLAGS="-static" LDFLAGS="-static"
}

function build_port() {
	make -j$NJOBS
}

function install_port() {
	make install DESTDIR=$SYSROOT
}
